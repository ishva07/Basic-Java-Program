// Convert Binary to Decimal

import java.util.*;

public class Main {
    public static void main(String[] args) {
     Scanner in = new Scanner(System.in);
     
     int n = in.nextInt();
     
     int i = 0,sum=0;
     
     while(n > 0){
       int rem = n % 10;
       sum+= (rem * Math.pow(2,i));
       n = n /10;
       i= i+1;
     }
     System.out.println(sum);
  }
}
